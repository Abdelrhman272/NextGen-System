<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sequence_fish_breeding_cycle" model="ir.sequence">
        <field name="name">Fish Breeding Cycle</field>
        <field name="code">fish.breeding.cycle</field>
        <field name="prefix">CYC/</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
    </record>
    
    <record id="sequence_fish_harvest" model="ir.sequence">
        <field name="name">Fish Harvest</field>
        <field name="code">fish.harvest</field>
        <field name="prefix">HAR/</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
    </record>
    
    <record id="picking_type_harvest" model="stock.picking.type">
        <field name="name">Harvest Transfer</field>
        <field name="code">internal</field>
        <field name="sequence_code">HAR</field>
        <field name="default_location_src_id" ref="stock.stock_location_locations"/>
        <field name="default_location_dest_id" ref="stock.stock_location_stock"/>
    </record>
</odoo>