<odoo>
    <record id="view_fish_farm_fishing_report_tree" model="ir.ui.view">
        <field name="name">fish.farm.fishing.report.tree</field>
        <field name="model">fish.farm.fishing</field>
        <field name="arch" type="xml">
            <list string="Fishing Report">
                <field name="date"/>
                <field name="pond_id"/>
                <field name="zone_id"/>
                <field name="sector_id"/>
                <field name="quantity_kg"/>
            </list>
        </field>
    </record>

    <record id="action_fish_farm_fishing_report" model="ir.actions.act_window">
        <field name="name">Fishing Report</field>
        <field name="res_model">fish.farm.fishing</field>
        <field name="view_mode">list</field>
        <field name="view_id" ref="view_fish_farm_fishing_report_tree"/>
    <field name="context">{'create': False}</field></record>

    <menuitem id="menu_fishfarm_fishing_report" name="Fishing Report" parent="menu_fishfarm_report_root" action="action_fish_farm_fishing_report"/>
<record id="view_fish_farm_fishing_report_search" model="ir.ui.view">
            <field name="name">fish.farm.fishing.report.search</field>
            <field name="model">fish.farm.fishing</field>
            <field name="arch" type="xml">
                <search>
                    <field name="date"/>
                    <field name="pond_id"/>
                </search>
            </field>
        </record></odoo>
