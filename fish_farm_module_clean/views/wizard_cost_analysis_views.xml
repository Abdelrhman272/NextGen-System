<odoo>
    <record id="view_fish_cost_analysis_wizard" model="ir.ui.view">
        <field name="name">fish.farm.cost.analysis.wizard.form</field>
        <field name="model">fish.farm.cost.analysis.wizard</field>
        <field name="arch" type="xml">
            <form string="تحليل التكاليف">
                <group>
                    <field name="date_from"/>
                    <field name="date_to"/>
                    <field name="pond_id"/>
                </group>
                <footer>
                    <button name="print_report" type="object" string="عرض التقرير" class="btn-primary"/>
                    <button string="إلغاء" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>