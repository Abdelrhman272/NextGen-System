<odoo>
    <record id="view_fish_farm_supplying_report_tree" model="ir.ui.view">
        <field name="name">fish.farm.supplying.report.tree</field>
        <field name="model">fish.farm.supplying</field>
        <field name="arch" type="xml">
            <tree string="Supplying Report">
                <field name="date"/>
                <field name="pond_id"/>
                <field name="zone_id"/>
                <field name="sector_id"/>
                <field name="customer_name"/>
                <field name="quantity_kg"/>
            </tree>
        </field>
    </record>

    <record id="action_fish_farm_supplying_report" model="ir.actions.act_window">
        <field name="name">Supplying Report</field>
        <field name="res_model">fish.farm.supplying</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="view_fish_farm_supplying_report_tree"/>
    </record>

    <menuitem id="menu_fishfarm_supplying_report" name="Supplying Report"
              parent="menu_fishfarm_report_root" action="action_fish_farm_supplying_report"/>
</odoo>