<odoo>
    <record id="view_fish_sector_tree" model="ir.ui.view">
        <field name="name">fish.farm.sector.tree</field>
        <field name="model">fish.farm.sector</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>

    <record id="view_fish_sector_form" model="ir.ui.view">
        <field name="name">fish.farm.sector.form</field>
        <field name="model">fish.farm.sector</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>