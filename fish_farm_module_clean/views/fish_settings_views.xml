<odoo>
  <record id="view_res_config_settings_tree" model="ir.ui.view">
    <field name="name">res.config.settings.tree</field>
    <field name="model">res.config.settings</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
      </tree>
    </field>
  </record>

  <record id="view_res_config_settings_form" model="ir.ui.view">
    <field name="name">res.config.settings.form</field>
    <field name="model">res.config.settings</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="action_res_config_settings" model="ir.actions.act_window">
    <field name="name">Settings</field>
    <field name="res_model">res.config.settings</field>
    <field name="view_mode">list,form</field>
  </record>

  <menuitem id="menu_res_config_settings" parent="menu_fish_farm_main" action="action_res_config_settings" sequence="10"/>
</odoo>
